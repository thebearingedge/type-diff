{
  "name": "type-diff",
  "version": "1.0.3",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint src/",
    "test": "mocha -r babel-register src/*.test.js",
    "tdd": "npm t -s -- -w",
    "coverage": "babel-node node_modules/.bin/babel-istanbul",
    "cover": "rimraf coverage/ && npm run -s coverage -- cover _mocha -- src/*.test.js",
    "transpile": "rimraf dist/ && babel -d dist/ src/",
    "precommit": "run-s lint cover transpile"
  },
  "files": [
    "dist/"
  ],
  "keywords": ["unit", "test", "types", "typeof", "structure"],
  "author": "Tim Davis",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/thebearingedge/type-diff.git"
  },
  "devDependencies": {
    "babel-cli": "6.14.0",
    "babel-eslint": "6.1.2",
    "babel-istanbul": "0.11.0",
    "babel-plugin-transform-export-extensions": "6.8.0",
    "babel-preset-es2015": "6.14.0",
    "chai": "3.5.0",
    "coveralls": "2.11.12",
    "eslint": "3.5.0",
    "husky": "0.11.7",
    "mocha": "3.0.2",
    "npm-run-all": "3.1.0",
    "rimraf": "2.5.4"
  }
}
